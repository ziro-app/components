{
  "name": "components",
  "version": "1.0.0",
  "description": "Component library for Ziro",
  "main": "index.js",
  "scripts": {
    "prestart": "node ./prebuild.js && bit link && bit build",
    "prebuild": "node ./prebuild.js && bit link && bit build",
    "postinstall": "npm dedupe",
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "test": "jest --coverage --watchAll false",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "test:cypress": "cypress open",
    "test:cypress:run": "cypress run",
    "test:cypress:ci": "start-server-and-test dev:base http-get://localhost:8080 test:cypress:run"
  },
  "keywords": [],
  "author": "Vitor Barbosa",
  "license": "ISC",
  "browserslist": [
    "last 1 Chrome version"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/App/**/*.js",
      "!**/mocks/**"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "test-config",
      "interfaces",
      "**/mocks/**"
    ]
  },
